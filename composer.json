{
    "name": "dimajolkin/ydb-doctrine",
    "license": "Apache-2.0",
    "description": "It's driver for YDB https://ydb.tech",
    "autoload": {
        "psr-4": {
            "Dimajolkin\\YdbDoctrine\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dimajolkin\\YdbDoctrine\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "dimajolkin",
            "email": "dimajolkin@yandex.ru"
        }
    ],
    "scripts": {
        "php-cs-fixer": "php-cs-fixer"
    },
    "require": {
        "php": "^8.1",
        "doctrine/orm": "^2.13",
        "ydb-platform/ydb-php-sdk": "^1.2"
    },
    "require-dev": {
        "symfony/var-dumper": "^6.1",
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^v3.0.0",
        "vimeo/psalm": "^5.15"
    }
}
